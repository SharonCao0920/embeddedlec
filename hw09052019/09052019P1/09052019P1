* FILENAME :       09052019P1            
*
* DESCRIPTION :
*       Write program to find sum of two numbers.
*       Output:
*       Enter two numbers: 5 6
*       sum=11
*
* AUTHOR :    Sharon Cao         DATE :  09/05/2019
*
*/

#include <stdio.h>

int main ()
{   //delear variables
	int num1, num2, sum; 
	
	//read inputs
	printf ("Enter two number: \n");
	scanf ("%d%d", &num1, &num2);
	
	//calculate total
	sum = num1 + num2;
	
	//print output
	printf ("Sum is %d", sum);
	
	return 0;
}

/*
result:
Enter two number:                                                                                                                                    
5                                                                                                                                                    
6                                                                                                                                                    
Sum is 11
*/




